[Unit]
Description=B2B App Auto IP Configuration and Startup
After=network-online.target docker.service
Wants=network-online.target
Requires=docker.service

[Service]
Type=oneshot
User=ubuntu
WorkingDirectory=/home/ubuntu/b2b-app
ExecStart=/home/ubuntu/b2b-app/scripts/startup.sh
RemainAfterExit=yes
StandardOutput=journal
StandardError=journal

[Install]
WantedBy=multi-user.target
